{
  "name": "api-rest",
  "version": "1.0.0",
  "description": "Creacion de una API-REST con Node.js, Express.js y Typescript and Testing",
  "main": "app.ts",
  "scripts": {
    "dev": "tsc && nodemon app.ts",
    "start": "tsc && node app.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test-watch": "jest --watch --detectOpenHandles --forceExit",
    "test-coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(ts|md)\""
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": ["dist"]
  },
  "repository": "https://github.com/leandroquiroga/API-REST-",
  "author": "Leandro Quiroga",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "mongodb-memory-server": "^8.11.5",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "jest": "^29.4.3",
    "mongoose": "^6.9.1",
    "nodemon": "^2.0.20",
    "pino": "^8.9.0",
    "pino-pretty": "^9.1.1"
  }
}
